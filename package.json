{
  "name": "huh-token",
  "version": "1.0.0",
  "description": "HUH token project",
  "main": "index.js",
  "scripts": {
    "hardhat": "node --require esm node_modules/.bin/hardhat",
    "flatten": "run(){ yarn hardhat flatten contracts/$1.sol > Flat$1.sol; }; run",
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test",
    "deploy": "yarn hardhat deploy --network",
    "coveralls": "yarn coverage && cat coverage/lcov.info | coveralls",
    "ganache:start": "ganache-cli",
    "solhint": "solhint 'contracts/**/*.sol'",
    "solhint:fix": "solhint --fix 'contracts/**/*.sol'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "gas-report": "env ENABLE_GAS_REPORT=1 npm run test"
  },
  "keywords": [
    "HUH"
  ],
  "author": "DsAtHuH",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@codechecks/client": "^0.1.12",
    "@ethersproject/hardware-wallets": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.3.3",
    "@openzeppelin/test-helpers": "^0.5.15",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "^1.7.0",
    "bn.js": "^5.2.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "esm": "^3.2.25",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "flatted": "^3.2.4",
    "hardhat": "^2.6.8",
    "hardhat-deploy": "^0.9.8",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-ethers": "^1.0.1",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.0",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "typescript": "^4.5.2",
    "web3": "^1.6.1"
  }
}
